{
    "name": "grad_case10",
    "ins": ["dA"],
    "outs": ["dB"],
    "data_type": "float",
    "kernel": "dB<10, 8>[i, j] = (select((i >= 0) && (i < 8), dA<8, 8>[i, j], 0.0) + select((i >= 1) && (i < 9), dA<8, 8>[i - 1, j], 0.0) + select((i >= 2) && (i < 10), dA<8, 8>[i - 2, j], 0.0)) / 3.0;",
    "grad_to": ["B"]
}
